{
    "name": "@camaro/redis",
    "version": "2.1.0",
    "main": "lib/index.js",
    "description": "Redis client for node, support resp3 and redis6.",
    "repository": {
        "type": "git",
        "url": "https://github.com/pikadun/redis"
    },
    "license": "MIT",
    "scripts": {
        "prepare": "husky install",
        "test": "ts-node test/index.ts",
        "coverage": "nyc --nycrc-path=./eng/.nycrc.json npm test",
        "lint": "eslint '**/*.ts'",
        "format": "ts-node ./eng/format.ts",
        "watch": "tsc --build ./eng/tsconfig.dev.json --watch",
        "compile": "tsc --build ./eng/tsconfig.prod.json",
        "gencmd": "ts-node ./eng/gencmd.ts"
    },
    "keywords": [],
    "types": "./types/index.d.ts",
    "eslintConfig": {
        "extends": "./eng/.eslintrc.json"
    },
    "devDependencies": {
        "@types/mocha": "^8.2.0",
        "@types/node": "^14.14.16",
        "@typescript-eslint/eslint-plugin": "^4.11.1",
        "@typescript-eslint/parser": "^4.11.1",
        "eslint": "^7.16.0",
        "husky": "^5.1.2",
        "mocha": "^8.2.1",
        "nyc": "^15.1.0",
        "ts-node": "^9.1.1",
        "typescript": "^4.1.3"
    }
}
